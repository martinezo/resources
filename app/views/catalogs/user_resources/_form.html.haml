= form_for @catalogs_user_resource do |f|
  - if @catalogs_user_resource.errors.any?
    #error_explanation
      %h2= "#{pluralize(@catalogs_user_resource.errors.count, "error")} prohibited this catalogs_user_resource from being saved:"
      %ul
        - @catalogs_user_resource.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :admin_user_id
    = f.number_field :admin_user_id
  .field
    = f.label :resource_id
    = f.number_field :resource_id
  .field
    = f.label :administrator
    = f.check_box :administrator
  .actions
    = f.submit 'Save'
