= form_for @agenda_reservation do |f|
  - if @agenda_reservation.errors.any?
    #error_explanation
      %h2= "#{pluralize(@agenda_reservation.errors.count, "error")} prohibited this agenda_reservation from being saved:"
      %ul
        - @agenda_reservation.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :folio
    = f.number_field :folio
  .field
    = f.label :requester
    = f.text_field :requester
  .field
    = f.label :email
    = f.text_field :email
  .field
    = f.label :phone
    = f.text_field :phone
  .field
    = f.label :foreing_headquarter_id
    = f.number_field :foreing_headquarter_id
  .field
    = f.label :event_type_id
    = f.number_field :event_type_id
  .field
    = f.label :status_id
    = f.number_field :status_id
  .field
    = f.label :comments
    = f.text_field :comments
  .field
    = f.label :modified_by
    = f.number_field :modified_by
  .actions
    = f.submit 'Save'
