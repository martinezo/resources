- es = 'activerecord.attributes.agenda/reservations.'
%table.table
  %tbody
    %tr
      %td.th
        = t("#{es}.folio")
      %td.tc
        = @agenda_reservation.folio
    %tr
      %td.th
        = t("#{es}.requester")
      %td.tc
        = @agenda_reservation.requester
    %tr
      %td.th
        = t("#{es}.email")
      %td.tc
        = @agenda_reservation.email
    %tr
      %td.th
        = t("#{es}.phone")
      %td.tc
        = @agenda_reservation.phone
    %tr
      %td.th
        = t("#{es}.foreign_headquarter_id")
      %td.tc
        = @agenda_reservation.catalogs_headquarter.name
    %tr
      %td.th
        = t("#{es}.event_type_id")
      %td.tc
        = @agenda_reservation.catalogs_event_type.name
    %tr
      %td.th
        = t("#{es}.status_id")
      %td.tc
        = @agenda_reservation.catalogs_status.name


.text-right
  = button_tag "<span class='fa fa-close'></span> #{t('commands.close')}".html_safe, data: { dismiss: 'modal'}, class: 'btn btn-default'

